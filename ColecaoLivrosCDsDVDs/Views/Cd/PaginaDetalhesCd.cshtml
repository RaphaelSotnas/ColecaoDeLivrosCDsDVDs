@model ColecaoLivrosCDsDVDs.Models.Entidades.CD

@{
    ViewData["Title"] = "PaginaDetalhesCd";
}

<h1>Detalhes do Cd</h1>

<div>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Nome)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Nome)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Cantor)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Cantor)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.GeneroMusical)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.GeneroMusical)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Status)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Status)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="PaginaEditarCd" asp-route-id="@Model.Id">Editar</a> |
    <a asp-action="ListarCds">Voltar</a>
</div>
